<query id="getRssList" action="select">
    <tables>
        <table name="livexe_rss" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" />
        <condition operation="equal" column="member_srl" var="member_srl" filter="number" pipe="and" />
        <condition operation="more" column="member_srl" var="s_member_srl" filter="number" pipe="and" />
        <condition operation="less" column="crawler_time" var="cur_time" filter="number" pipe="and" />
    </conditions>
    <navigation>
        <index var="sort_index" default="regdate" order="order_type" />
        <list_count var="list_count" default="20" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>
